@using TransportManagementWeb.Global
<!DOCTYPE html>
<html class="no-js">
<meta http-equiv="content-type" content="text/html;charset=utf-8" />
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Demon International</title>
    <meta name="theme-color" content="#ffffff">

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,700,600,300" rel="stylesheet" type="text/css">

    <link rel="stylesheet" href="~/styles/bootstrap.css">

    <link rel="stylesheet" href="~/styles/dependencies.css">

    <link rel="stylesheet" href="~/styles/components.css">

    <link rel="stylesheet" href="~/styles/wrapkit.css">

    <link rel="stylesheet" href="~/styles/demo.css">
    <link href="~/styles/jquery-ui.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
</head>
<body>
    <main class="wrapkit-wrapper" id="wrapper">

        <!-- ============================================
        HEADER SECTION
        =============================================== -->
        <!-- navigation -->
        <nav class="header navbar">
            <div class="container-fluid">
                <!-- use .pull-*, couse we need this float on any screen size -->
                <div class="pull-left">
                    <a class="navbar-brand" href="~/Masters/Dashboard" role="logo" aria-label="Logo">
                        <img src="~/images/logo/logo.png" style="height:40px; width:178px;"/>
                        <img class="logo hidden" src="~/images/logo/logo.png" alt="Brand" style="height:42px; width:178px;">
                    </a>
                    <button data-sidebar="toggleVisible" class="btn btn-icon navbar-btn">
                        <i class="fa fa-bars"></i>
                        <i class="fa fa-caret-down"></i>
                    </button>
                </div>

                <!-- use .pull-*, couse we need this float on any screen size -->
                <div class="pull-right" role="navigation">

                    <div class="dropdown">

                        <br />
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <label id="lblUserName1" runat="server"></label>
                            <i class="fa fa-user fa-fw"></i>  <i class="fa fa-caret-down"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-menu-right" role="menu">
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="page-profile.html"><span class="pull-right"><i class="fa fa-user text-muted"></i></span>Profile</a></li>
                            <li class="divider"></li>
                            <li role="presentation"><a data-toggle="modal" data-target="#customModal2" role="menuitem" tabindex="-1"><span class="pull-right"><i class="fa fa-key text-muted"></i></span>Change Password</a></li>
                            <li class="divider"></li>
                            <li role="presentation"><a id="A1" role="menuitem" tabindex="-1"><span class="pull-right"><i class="fa fa-sign-out text-muted"></i></span> Sign out</a></li>
                            <li class="divider"></li>
                        </ul>
                    </div><!-- /.dropdown -->
                </div><!-- /navigation -->

            </div><!-- /.container-fluid -->
        </nav><!-- /navigation -->
        <!-- ============================================
        SIDEBAR SECTION
        =============================================== -->
        <aside class="sidebar" role="complementary">

            <!-- profile -->
            <div class="sidebar-block">
                <div class="media">
                    <div class="media-left">
                        <a href="~/Masters/Dashboard">
                            <span class="status dotted dotted-primary" data-toggle="tooltip" data-container="body" title="available"></span>
                            <img class="media-object img-circle" src="~/images/dummy/unknown-profile.jpg" alt="photo profile">
                        </a>
                    </div>
                    <div class="media-body">
                        <h4 class="media-heading" id="lblUserName" runat="server">  </h4>

                    </div>
                </div>
            </div><!-- /.sidebar-block -->
            <!-- /profile -->
            <!-- /navigation -->
            <div class="nav-wrapper">
                <ul class="nav nav-stacked" role="navigation">
                    <li class="nav-item open active" role="presentation">
                        <a href="~/Masters/Dashboard" class="active">
                            <span class="nav-icon"><i class="fa fa-dashcube"></i></span>
                            <span class="nav-text">Dashboard</span>
                        </a>
                        <ul class="nav nav-child level-1 nav-stacked nav-pills" role="menu">
                            <li role="presentation" class="clientPageActive"><a role="menuitem" tabindex="-1" href="~/Masters/ClientMaster"><span class="nav-text">Client Master</span></a></li>
                            <li role="presentation" class="vendorPageActive"><a role="menuitem" tabindex="-1" href="~/Masters/VendorMaster"><span class="nav-text">Vendor Master</span></a></li>
                        </ul>
                    </li>

                    <li class="divider"></li>
                </ul>
            </div>
        </aside><!-- /.SIDEBAR -->
        <!-- ============================================
        MAIN CONTENT SECTION
        =============================================== -->
        @RenderBody()
        <!-- ============================================
        FOOTER SECTION
        =============================================== -->
        <footer class="footer-wrapper" role="contentinfo">
            <div class="footer">
                <div class="pull-right text-muted"><small>Currently v1.2</small></div>
                <div>Wrapkit Template  &copy; 2015</div>
            </div>
        </footer><!-- /.FOOTER -->

    </main><!-- /#MAIN -->
    
    <script src="~/scripts/vendor.js"></script>
    <script src="~/scripts/dependencies.js"></script>
    <script src="~/scripts/wrapkit.js"></script>
    <script src="~/scripts/wrapkit-setup.js"></script>
    <script src="~/scripts/plugin-setups.js"></script>
    @*<script src="~/scripts/morris.bundle.js"></script>*@
    <script src="~/scripts/jquery.sparkline.js"></script>
    <script src="~/scripts/jquery.easypiechart.js"></script>
    @*<script src="~/scripts/demo/dashboard1-demo.js"></script>*@
    @if (TempData["Alert_Title"] != null)
    {
        <div id="dialog" title="@TempData["Alert_Title"].ToString()">
            @if (TempData["Alert_Message"] != null)
            {
                <p>@TempData["Alert_Message"].ToString()</p>
            }
            else
            {
                <p>Success..!</p>
            }
        </div>
    }
    @*Alert*@
    <div id="alertModel" style="display:none;" title="">
        <p></p>
    </div>

    <script>
        $(function () {
            $(document).ready(function () {
                $("#dialog").dialog({
                    resizable: false,
                    height: "auto",
                    width: 400,
                    modal: true,
                    buttons: {
                        "OK": function () {
                            $(this).dialog("close");
                        }
                    }
                });
            })
        });
    </script>
</body>
</html>
